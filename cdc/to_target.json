{
  "name": "jdbc-sink",
  "config": {
    "connector.class": "io.debezium.connector.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "to-replication-customer",
	"collection.name.format": "testcontainers.customers",
	"primary.key.mode": "record_value",
	"primary.key.fields": "id",
    "connection.url": "jdbc:postgresql://postgres_target:5432/customer_target_db",
    "connection.username": "user",
    "connection.password": "123456",
	"slot.name": "to_source_connector_slot",
    "insert.mode": "insert",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "true",
	"transforms":"timestamp",
	"transforms.timestamp.type":"org.apache.kafka.connect.transforms.TimestampConverter$Value",
	"transforms.timestamp.target.type":"Timestamp",
	"transforms.timestamp.field":"created_at",
	"transforms.timestamp.format":"yyyy-MM-dd HH:mm:ss.SSS",
	"errors.log.enable": "true"
  }
}